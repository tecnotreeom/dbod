{
	"expo": {
		"name": "dbod",
		"slug": "dbod",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/appstore.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff",
			"flex": 1
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.omgupta.dbod"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/playstore.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.omgupta.dbod",
			"useNextNotificationsApi": true,
			"googleServicesFile": "./google-services.json"
		},
		"web": {
			"favicon": "./assets/playstore.png"
		},
		"plugins": [
			[
				"expo-notifications",
				{
					"icon": "./assets/playstore.png",
					"color": "#ffffff"
				}
			]
		]
	}
}
